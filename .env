# CONFIGURAÇÕES DO DETECTOR INTELIGENTE v7.3.4-resumo-detalhado
# ===================================================================
# Este arquivo contém todos os parâmetros configuráveis da aplicação
# Modifique os valores conforme necessário para ajustar a sensibilidade

# ===================================================================
# CONFIGURAÇÕES GERAIS DA APLICAÇÃO
# ===================================================================

# Título da aplicação exibido na interface
VITE_APP_TITLE="Detector Inteligente"

# Versão da aplicação exibida no rodapé
VITE_APP_VERSION=v7.3.4-swagger-completo

# Tamanho máximo de arquivo em MB (padrão: 10MB)
VITE_MAX_FILE_SIZE_MB=10

# ===================================================================
# CONFIGURAÇÕES DE DETECÇÃO NSFW
# ===================================================================

# Threshold para classificar como pornografia (0.0-1.0, padrão: 0.5)
# Valores menores = mais sensível, mais detecções
# Valores maiores = menos sensível, menos falsos positivos
VITE_NSFW_PORN_THRESHOLD=0.5

# Threshold para classificar como hentai (0.0-1.0, padrão: 0.5)
VITE_NSFW_HENTAI_THRESHOLD=0.5

# Threshold para classificar como conteúdo sexy (0.0-1.0, padrão: 0.7)
# Valor mais alto pois "sexy" pode incluir conteúdo menos explícito
VITE_NSFW_SEXY_THRESHOLD=0.7

# Threshold para considerar "sexy" como destrutivo na interface (0.0-1.0, padrão: 0.5)
VITE_NSFW_SEXY_DESTRUCTIVE_THRESHOLD=0.5

# ===================================================================
# CONFIGURAÇÕES DE DETECÇÃO DE JOGOS
# ===================================================================

# Threshold principal para detecção de jogos (0.0-1.0, padrão: 0.3)
# Valores menores = mais jogos detectados, mais falsos positivos
# Valores maiores = menos falsos positivos, pode perder alguns jogos
VITE_GAME_DETECTION_THRESHOLD=0.3

# Threshold para considerar "Drawing" como indicativo de jogo (0.0-1.0, padrão: 0.6)
# Drawing alto geralmente indica gráficos de jogos ou arte digital
VITE_GAME_DRAWING_THRESHOLD=0.6

# Threshold para detecção de software (0.0-1.0, padrão: 0.25)
# Usado para detectar interfaces de software e aplicações
VITE_SOFTWARE_DETECTION_THRESHOLD=0.25

# ===================================================================
# CONFIGURAÇÕES AVANÇADAS DE DETECÇÃO DE JOGOS
# ===================================================================

# Pesos para diferentes aspectos da detecção de jogos (0.0-1.0)
VITE_GAME_COLOR_WEIGHT=0.25
VITE_GAME_PATTERN_WEIGHT=0.20
VITE_GAME_UI_WEIGHT=0.30
VITE_GAME_TEXTURE_WEIGHT=0.15
VITE_GAME_SATURATION_WEIGHT=0.10

# Threshold de saturação para considerar como jogo (0.0-1.0, padrão: 0.7)
VITE_GAME_SATURATION_THRESHOLD=0.7

# Bonus por alta saturação (0.0-1.0, padrão: 0.1)
VITE_GAME_SATURATION_BONUS=0.1

# ===================================================================
# CONFIGURAÇÕES DE REGIÕES HUD (HEADS-UP DISPLAY)
# ===================================================================

# Coordenadas das regiões HUD para análise de jogos (0.0-1.0)
# Top-left (canto superior esquerdo)
VITE_HUD_TOP_LEFT_X=0.0
VITE_HUD_TOP_LEFT_Y=0.0
VITE_HUD_TOP_LEFT_WIDTH=0.25
VITE_HUD_TOP_LEFT_HEIGHT=0.15

# Top-right (canto superior direito)
VITE_HUD_TOP_RIGHT_X=0.75
VITE_HUD_TOP_RIGHT_Y=0.0
VITE_HUD_TOP_RIGHT_WIDTH=0.25
VITE_HUD_TOP_RIGHT_HEIGHT=0.15

# Bottom-left (canto inferior esquerdo)
VITE_HUD_BOTTOM_LEFT_X=0.0
VITE_HUD_BOTTOM_LEFT_Y=0.85
VITE_HUD_BOTTOM_LEFT_WIDTH=0.25
VITE_HUD_BOTTOM_LEFT_HEIGHT=0.15

# Bottom-right (canto inferior direito)
VITE_HUD_BOTTOM_RIGHT_X=0.75
VITE_HUD_BOTTOM_RIGHT_Y=0.85
VITE_HUD_BOTTOM_RIGHT_WIDTH=0.25
VITE_HUD_BOTTOM_RIGHT_HEIGHT=0.15

# Bottom-center (centro inferior - barra de habilidades)
VITE_HUD_BOTTOM_CENTER_X=0.3
VITE_HUD_BOTTOM_CENTER_Y=0.8
VITE_HUD_BOTTOM_CENTER_WIDTH=0.4
VITE_HUD_BOTTOM_CENTER_HEIGHT=0.2

# ===================================================================
# PESOS DAS REGIÕES HUD
# ===================================================================

# Peso de cada região HUD na análise (0.0-1.0)
VITE_HUD_TOP_LEFT_WEIGHT=0.15
VITE_HUD_TOP_RIGHT_WEIGHT=0.20
VITE_HUD_BOTTOM_LEFT_WEIGHT=0.10
VITE_HUD_BOTTOM_RIGHT_WEIGHT=0.15
VITE_HUD_BOTTOM_CENTER_WEIGHT=0.40

# ===================================================================
# CONFIGURAÇÕES DE ANÁLISE DE OCIOSIDADE
# ===================================================================

# Threshold para considerar uma mudança como significativa (0-255, padrão: 15)
# Valores menores = mais sensível a mudanças pequenas
# Valores maiores = apenas mudanças grandes são consideradas
VITE_IDLENESS_CHANGE_THRESHOLD=15

# Threshold para considerar como alta ociosidade (0-100, padrão: 70)
VITE_IDLENESS_HIGH_THRESHOLD=70

# Threshold para considerar como ociosidade moderada (0-100, padrão: 40)
VITE_IDLENESS_MODERATE_THRESHOLD=40

# Resolução máxima para análise de ociosidade (pixels, padrão: 800)
# Imagens maiores são redimensionadas para melhorar performance
VITE_IDLENESS_MAX_RESOLUTION=800

# ===================================================================
# CONFIGURAÇÕES DE ANÁLISE TEMPORAL
# ===================================================================

# Thresholds para classificação de atividade (0-100)
VITE_TIME_VERY_LOW_ACTIVITY_THRESHOLD=1
VITE_TIME_LOW_ACTIVITY_THRESHOLD=15
VITE_TIME_MODERATE_ACTIVITY_THRESHOLD=30
VITE_TIME_HIGH_ACTIVITY_THRESHOLD=50

# Thresholds para análise de produtividade (0-100)
VITE_TIME_PRODUCTIVE_THRESHOLD=60
VITE_TIME_UNPRODUCTIVE_THRESHOLD=30

# ===================================================================
# CONFIGURAÇÕES DE BORRAMENTO AUTOMÁTICO
# ===================================================================

# Habilitar borramento automático para jogos (true/false)
VITE_BLUR_GAMES=true

# Habilitar borramento automático para conteúdo NSFW (true/false)
VITE_BLUR_NSFW=true

# Nível de borramento para jogos (light, medium, heavy, extreme)
VITE_GAME_BLUR_LEVEL=light

# Nível de borramento para conteúdo sexy (light, medium, heavy, extreme)
VITE_SEXY_BLUR_LEVEL=medium

# Nível de borramento para pornografia (light, medium, heavy, extreme)
VITE_PORN_BLUR_LEVEL=heavy

# Nível de borramento para hentai (light, medium, heavy, extreme)
VITE_HENTAI_BLUR_LEVEL=extreme

# Mostrar marca d'água de evidência (true/false)
VITE_SHOW_EVIDENCE_WATERMARK=true

# Mostrar marcadores coloridos nos cantos (true/false)
VITE_SHOW_BLUR_MARKERS=true

# ===================================================================
# CONFIGURAÇÕES DE INTERFACE
# ===================================================================

# Número máximo de resultados NSFW a exibir (padrão: 5)
VITE_MAX_NSFW_RESULTS=5

# Número máximo de jogos detectados a exibir (padrão: 3)
VITE_MAX_GAMES_DETECTED=3

# Número máximo de URLs a exibir (padrão: 10)
VITE_MAX_URLS_DISPLAY=10

# Número máximo de software detectado a exibir (padrão: 5)
VITE_MAX_SOFTWARE_DISPLAY=5

# Número máximo de logs de detecção a exibir (padrão: 20)
VITE_MAX_DETECTION_LOGS=20

# Número máximo de horas produtivas a exibir (padrão: 5)
VITE_MAX_PRODUCTIVE_HOURS=5

# Número máximo de horas improdutivas a exibir (padrão: 5)
VITE_MAX_UNPRODUCTIVE_HOURS=5

# ===================================================================
# CONFIGURAÇÕES DE DEBUG
# ===================================================================

# Habilitar logs detalhados no console (true/false)
VITE_DEBUG_ENABLED=false

# Habilitar logs de detecção de jogos (true/false)
VITE_DEBUG_GAME_DETECTION=false

# Habilitar logs de análise de ociosidade (true/false)
VITE_DEBUG_IDLENESS_ANALYSIS=false

# Habilitar logs de OCR (true/false)
VITE_DEBUG_OCR=false

# ===================================================================
# CONFIGURAÇÕES EXPERIMENTAIS
# ===================================================================

# Habilitar detecção experimental de padrões (true/false)
VITE_EXPERIMENTAL_PATTERNS=false

# Habilitar análise de áudio (quando disponível) (true/false)
VITE_EXPERIMENTAL_AUDIO=false

# Multiplicador de sensibilidade experimental (0.5-2.0, padrão: 1.0)
VITE_EXPERIMENTAL_SENSITIVITY=1.0
